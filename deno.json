{
  "tasks": {
    "start": "deno run --watch --allow-net --allow-read main.ts"
  },
  "imports": {
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.4.3",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "hono": "npm:hono@^4.7.1",
    "nano-jsx": "https://deno.land/x/nano_jsx/mod.ts",
    "nano/jsx-runtime": "https://deno.land/x/nano_jsx/jsx-runtime/index.ts",
    "zod": "npm:zod@^3.24.2"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "nano",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "fmt": {
    "semiColons": false
  }
}
